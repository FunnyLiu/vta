{
  "vta": {
    "dirs": {
      "src": "source-code"
    },
    "config": {
      "build-plugin": {
        "esnext": {
          "modules": false
        },
        "typescript": {
          "jsx": false
        }
      }
    },
    "env": {
      "locale": {
        "dirs": {
          "build": "dist-locale"
        },
        "config": {
          "build-plugin": {
            "typescript": {
              "jsx": true
            }
          }
        },
        "plugins": [
          [
            "./plugins/locale"
          ]
        ]
      },
      "server": {
        "dirs": {
          "build": "dist-server"
        },
        "config": {
          "build-plugin": {
            "esnext": {
              "modules": "commonjs"
            }
          }
        },
        "plugins": [
          [
            "./plugins/server"
          ]
        ]
      }
    }
  }
}
