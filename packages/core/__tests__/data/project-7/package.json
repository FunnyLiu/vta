{
  "vta": {
    "dirs": {
      "src": "source-code"
    },
    "config": {
      "esnext": {
        "modules": false
      },
      "typescript": {
        "jsx": false
      }
    },
    "env": {
      "locale": {
        "dirs": {
          "build": "dist-locale"
        },
        "config": {
          "typescript": {
            "jsx": true
          }
        },
        "plugins": [
          [
            "./plugins/locale"
          ]
        ]
      },
      "server": {
        "dirs": {
          "build": "dist-server"
        },
        "config": {
          "esnext": {
            "modules": "commonjs"
          }
        },
        "plugins": [
          [
            "./plugins/server"
          ]
        ]
      }
    }
  }
}
